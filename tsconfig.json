{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"outDir": "./dist",
		"resolveJsonModule": true,
		"removeComments": true,
		"noUnusedLocals": true
	},
	"include": ["src"]
}
